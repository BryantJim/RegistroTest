        <TextBlock HorizontalAlignment="Left" Height="27" Margin="80,85,0,0" Text="Estudiante Id" TextWrapping="Wrap" VerticalAlignment="Top" Width="103"/>
        <Button Content="Atrás" HorizontalAlignment="Left" Height="33" Margin="62,24,0,0" VerticalAlignment="Top" Width="67" AutomationProperties.IsOffscreenBehavior="Default" RenderTransformOrigin="0.5,0.5" HorizontalContentAlignment="Center" ScrollViewer.HorizontalScrollBarVisibility="Visible" VerticalContentAlignment="Center" Click="Button_Click" Background="#FFFFFBFB" FlowDirection="RightToLeft" Padding="1,1,15,1"/>

